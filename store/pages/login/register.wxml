<form bindsubmit='saveMerchant'>
	<view class="weui-cells__title">超级管理员</view>
	<view class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">姓名</view>
			</view>
			<view class="weui-cell__bd">
				<input name="contactName" value="{{contactName}}" class="weui-input" placeholder="请输入管理员姓名" maxlength="8" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">身份证</view>
			</view>
			<view class="weui-cell__bd">
				<input name="contactIdNumber" value="{{contactIdNumber}}" class="weui-input" placeholder="请输入管理员身份证号" maxlength="18" type="number" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">手机号</view>
			</view>
			<view class="weui-cell__bd">
				<input name="mobilePhone" value="{{mobilePhone}}" class="weui-input" placeholder="请输入管理员手机号" maxlength="11" type="number" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">常用邮箱</view>
			</view>
			<view class="weui-cell__bd">
				<input name="contactEmail" value="{{contactEmail}}" class="weui-input" placeholder="请输入常用邮箱" />
			</view>
		</view>
	</view>
	<view class="weui-cells__title">营业执照</view>
	<view class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">主体类型</view>
			</view>
			<view class="weui-cell__bd">
				<radio-group class="weui-input radio-group" bindchange="radioChange" id='subjectType'>
					<label class="radio">
						<radio value="SUBJECT_TYPE_INDIVIDUAL" checked="{{subjectType == 'SUBJECT_TYPE_INDIVIDUAL'}}" />个体 </label>
					<label class="radio">
						<radio value="SUBJECT_TYPE_ENTERPRISE" checked="{{subjectType == 'SUBJECT_TYPE_ENTERPRISE'}}" />企业 </label>
				</radio-group>
			</view>
		</view>
		<image wx:if="{{licenseCopy == null}}" src="/images/五证合一.png" mode="aspectFit" style="width:100%" bindtap="chooseImage" id="licenseCopy"></image>
		<icon wx:if="{{licenseCopy!=null}}" type="cancel" bindtap="deletePicture" id="licenseCopy"></icon>
		<image wx:if="{{licenseCopy!=null}}" src="{{licenseCopy}}" mode="aspectFit" style="width:100%"></image>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">社会信用代码</view>
			</view>
			<view class="weui-cell__bd">
				<input id="licenseNumber" name="licenseNumber" value="{{licenseNumber}}" class="weui-input" placeholder="18位统一社会信用代码" maxlength="18" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">商户全称</view>
			</view>
			<view class="weui-cell__bd">
				<input name="lmerchantName" value="{{merchantName}}" class="weui-input" placeholder="请输入营业执照名称" maxlength="10" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">经营者</view>
			</view>
			<view class="weui-cell__bd">
				<input name="legalPerson" value="{{legalPerson}}" class="weui-input" placeholder="法人姓名" maxlength="8" />
			</view>
		</view>
	</view>
	<view class="weui-cells__title">法人信息</view>
	<view class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<image wx:if="{{idCardCopy == null}}" src="/images/身份证正面.png" mode="aspectFit" style="width:100%" bindtap="chooseImage" id="idCardCopy"></image>
		<icon wx:if="{{idCardCopy!=null}}" type="cancel" bindtap="deletePicture" id="idCardCopy"></icon>
		<image wx:if="{{idCardCopy!=null}}" src="{{idCardCopy}}" mode="aspectFit" style="width:100%"></image>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">法人证件号</view>
			</view>
			<view class="weui-cell__bd">
				<input name="idCardNumber" value="{{idCardNumber}}" class="weui-input" placeholder="请输入法人18位身份证号" maxlength="18" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">法人姓名</view>
			</view>
			<view class="weui-cell__bd">
				<input name="idCardName" value="{{idCardName}}" class="weui-input" placeholder="请输入法人姓名" maxlength="18" />
			</view>
		</view>
		<image wx:if="{{idCardNational == null}}" src="/images/身份证背面.png" mode="aspectFit" style="width:100%" bindtap="chooseImage" id="idCardNational"></image>
		<icon wx:if="{{idCardNational!=null}}" type="cancel" bindtap="deletePicture" id="idCardNational"></icon>
		<image wx:if="{{idCardNational!=null}}" src="{{idCardNational}}" mode="aspectFit" style="width:100%"></image>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">开始时间</view>
			</view>
			<view class="weui-cell__bd">
				<picker mode="date" bindchange="datePickerChange" id="cardPeriodBegin">
					<input name="cardPeriodBegin" value="{{cardPeriodBegin}}" class="weui-input" placeholder="身份证登记时间" disabled="true" />
				</picker>
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">结束时间</view>
			</view>
			<view class="weui-cell__bd">
				<picker mode="date" bindchange="datePickerChange" id="cardPeriodEnd">
					<input name="cardPeriodEnd" value="{{cardPeriodEnd}}" class="weui-input" placeholder="身份证有效截止时间" disabled="true" />
				</picker>
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">是否是最终受益人</view>
			</view>
			<view class="weui-cell__ft">
				<switch id="owner" checked="{{owner}}" bindchange="switchChange" />
			</view>
		</view>
	</view>
	<view wx:if="{{!owner}}" class="weui-cells__title">最终受益人 </view>
	<view wx:if="{{!owner}}" class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<image wx:if="{{idCardA == null}}" src="/images/身份证正面.png" mode="aspectFit" style="width:100%" bindtap="chooseImage" id="idCardA"></image>
		<icon wx:if="{{idCardA!=null}}" type="cancel" bindtap="deletePicture" id="idCardA"></icon>
		<image wx:if="{{idCardA!=null}}" src="{{idCardA}}" mode="aspectFit" style="width:100%"></image>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">法人证件号</view>
			</view>
			<view class="weui-cell__bd">
				<input name="idNumber" value="{{idNumber}}" class="weui-input" placeholder="法人18位身份证号" maxlength="18" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">法人姓名</view>
			</view>
			<view class="weui-cell__bd">
				<input name="beneficiary" value="{{beneficiary}}" class="weui-input" placeholder="请输入姓名" maxlength="18" />
			</view>
		</view>
		<image wx:if="{{idCardB == null}}" src="/images/身份证背面.png" mode="aspectFit" style="width:100%" bindtap="chooseImage" id="idCardB"></image>
		<icon wx:if="{{idCardB!=null}}" type="cancel" bindtap="deletePicture" id="idCardB"></icon>
		<image wx:if="{{idCardB!=null}}" src="{{idCardB}}" mode="aspectFit" style="width:100%"></image>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">开始时间</view>
			</view>
			<view class="weui-cell__bd">
				<picker mode="date" bindchange="datePickerChange" id="idPeriodBegin">
					<input name="idPeriodBegin" value="{{idPeriodBegin}}" class="weui-input" placeholder="身份证登记时间" disabled="true" />
				</picker>
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">结束时间</view>
			</view>
			<view class="weui-cell__bd">
				<picker mode="date" bindchange="datePickerChange" id="idPeriodEnd">
					<input name="idPeriodEnd" value="{{idPeriodEnd}}" class="weui-input" placeholder="身份证有效截止时间" disabled="true" />
				</picker>
			</view>
		</view>
	</view>
	<view class="weui-cells__title">经营资料</view>
	<view class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">商户简称</view>
			</view>
			<view class="weui-cell__bd">
				<input name="merchantShortname" value="{{merchantShortname}}" class="weui-input" placeholder="请输入商户简称" maxlength="8" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">客服电话</view>
			</view>
			<view class="weui-cell__bd">
				<input name="servicePhone" value="{{servicePhone}}" class="weui-input" placeholder="请输入客服电话" maxlength="8" />
			</view>
		</view>
		<view class="weui-cell">
			<view class="weui-cell__bd">
				<view class="weui-uploader">
					<view class="weui-uploader__hd">
						<view class="weui-uploader__overview">
							<view class="weui-uploader__title">小程序截图</view>
							<view class="weui-uploader__info">{{miniProgramPics.length}}/5 <icon bindtap="clearPictues" id="miniProgramPics" type="clear" size="21"></icon>
							</view>
						</view>
					</view>
					<view class="weui-uploader__bd">
						<view class="weui-uploader__files" id="uploaderFiles">
							<block wx:for="{{miniProgramPics}}" wx:key="*this">
								<view class="weui-uploader__file" id="{{item}}">
									<icon type="cancel" bindtap="deletePictures" data-index="{{index}}" id="miniProgramPics"></icon>
									<image class="weui-uploader__img" src="{{item}}" mode="aspectFit" />
								</view>
							</block>
						</view>
						<view wx:if="{{miniProgramPics.length<5}}" class="weui-uploader__input-box">
							<view class="weui-uploader__input" bindtap="chooseImages" id="miniProgramPics" data-count="5"></view>
						</view>
					</view>
				</view>
			</view>
		</view>
	</view>
	<view class="weui-cells__title">结算规则</view>
	<view class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">所属行业</view>
			</view>
			<view class="weui-cell__bd">
				<picker bindchange="pickerChange" range="{{qualification[subjectType]}}" id="qualificationType" value="{{qualificationType}}" range-key="name">
					<view>{{qualificationType?qualification[subjectType][qualificationType].name:'请选择所属行业'}}</view>
				</picker>
			</view>
		</view>
		<view class="weui-cell">
			<view class="weui-cell__bd">
				<view class="weui-uploader">
					<view class="weui-uploader__hd">
						<view class="weui-uploader__overview">
							<view class="weui-uploader__title">特殊资质图片</view>
							<view class="weui-uploader__info">{{qualifications.length}}/5 <icon bindtap="clearPictues" id="qualifications" type="clear" size="21"></icon>
							</view>
						</view>
					</view>
					<view class="weui-uploader__bd">
						<view class="weui-uploader__files" id="uploaderFiles">
							<block wx:for="{{qualifications}}" wx:key="*this">
								<view class="weui-uploader__file" id="{{item}}">
									<icon type="cancel" bindtap="deletePictures" id="qualifications"></icon>
									<image class="weui-uploader__img" src="{{item}}" mode="aspectFill" />
								</view>
							</block>
						</view>
						<view wx:if="{{qualifications.length<5}}" class="weui-uploader__input-box">
							<view class="weui-uploader__input" bindtap="chooseImages" id="qualifications" data-count="5"></view>
						</view>
					</view>
				</view>
			</view>
		</view>
	</view>
	<view class="weui-cells__title">结算银行账户</view>
	<view class="weui-cells weui-cells_after-title" style="font-size:27rpx">
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">账户类型</view>
			</view>
			<view class="weui-cell__bd">
				<radio-group class="weui-input radio-group" bindchange="radioChange" id="bankAccountType">
					<label class="radio"></label>
					<radio value="BANK_ACCOUNT_TYPE_PERSONAL" checked="{{bankAccountType == 'BANK_ACCOUNT_TYPE_PERSONAL'}}" />个人
					<label class="radio"></label>
					<radio value="BANK_ACCOUNT_TYPE_CORPORATE" checked="{{bankAccountType == 'BANK_ACCOUNT_TYPE_CORPORATE'}}" />公户
				</radio-group>
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">开户名称</view>
			</view>
			<view class="weui-cell__bd">
				<input name="accountName" value="{{accountName}}" class="weui-input" placeholder="请填写开户名称" />
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">开户银行</view>
			</view>
			<view class="weui-cell__bd">
				<picker bindchange="pickerChange" range="{{banks}}" id="accountBank" value="{{accountBank}}">
					<view>{{accountBank?accountBank:'请选择开户行'}}</view>
				</picker>
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">开户地址</view>
			</view>
			<view class="weui-cell__bd">
				<picker id="bankAddress" bindchange="regionChange" mode="region">
					<view class="weui-input">{{bankAddress.value.length===3?{...bankAddress.value}:'请选择开户地址'}}</view>
				</picker>
			</view>
		</view>
		<view class="weui-cell ">
			<view class="weui-cell__hd">
				<view class="weui-label">银行账号</view>
			</view>
			<view class="weui-cell__bd">
				<input name="accountNumber" value="{{accountNumber}}" class="weui-input" placeholder="请填写银行账号" />
			</view>
		</view>
		<view class="weui-cell">
			<view class="weui-cell__hd">
				<view class="weui-label">银行全称</view>
			</view>
			<view class="weui-cell__bd">
				<input name="bankNamee" value="{{bankName}}" class="weui-input" placeholder='请填写银行全称' />
			</view>
		</view>
	</view>
	<view class="weui-cell">
		<button style="weui-btn" type="default" formType="reset">重置</button>
		<button style="weui-btn" type="primary" formType="submit">保存</button>
	</view>
</form>