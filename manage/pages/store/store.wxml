<view class="weui-cells__title">特约商户管理</view>
<view class="weui-panel">
	<view class="weui-cells weui-cells_after-title">
		<navigator class="weui-cell" url='/pages/store/list?state=EDITTING'>
			<view class="weui-cell__hd">
				<image src="/images/watting-pay.png" style="height:1em;width:1em;margin-right: 16px;" />
			</view>
			<view class="weui-cell__bd">商户编辑中</view>
			<view class="weui-cell__ft weui-cell__ft_in-access"></view>
		</navigator>
		<navigator class="weui-cell" url='/pages/store/list?state=SYSTEM_AUDITING'>
			<view class="weui-cell__hd">
				<image mode="aspectFit" src="/images/watting-package.png" style="height:1em;width:1em;margin-right: 16px;" />
			</view>
			<view class="weui-cell__bd">平台待审核</view>
			<view class="weui-cell__ft weui-cell__ft_in-access"></view>
		</navigator>

		<navigator class="weui-cell" url='/pages/store/list?state=WEACHT_AUDITING'>
			<view class="weui-cell__hd">
				<image mode="aspectFit" src="/images/watting-deliver.png" style="height:1em;width:1em;margin-right: 16px;" />
			</view>
			<view class="weui-cell__bd">微信审核中</view>
			<view class="weui-cell__ft weui-cell__ft_in-access"></view>
		</navigator>

		<navigator class="weui-cell" url='/pages/store/list?state=REJECTED'>
			<view class="weui-cell__hd">
				<image mode="aspectFit" src="/images/watting-sign.png" style="height:1em;width:1em;margin-right: 16px;" />
			</view>
			<view class="weui-cell__bd">审核失败</view>
			<view class="weui-cell__ft weui-cell__ft_in-access"></view>
		</navigator>
		<navigator class="weui-cell" url='/pages/store/list?state=FINISHED'>
			<view class="weui-cell__hd">
				<image mode="aspectFit" src="/images/finish.png" style="height:1em;width:1em;margin-right: 16px;" />
			</view>
			<view class="weui-cell__bd">入住成功</view>
			<view class="weui-cell__ft weui-cell__ft_in-access"></view>
		</navigator>

			<navigator class="weui-cell" url='/pages/store/list?state=CANCELED'>
			<view class="weui-cell__hd">
				<image mode="aspectFit" src="/images/finish.png" style="height:1em;width:1em;margin-right: 16px;" />
			</view>
			<view class="weui-cell__bd">撤销申请</view>
			<view class="weui-cell__ft weui-cell__ft_in-access"></view>
		</navigator>

	</view>
</view>