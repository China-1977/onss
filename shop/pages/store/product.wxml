<view class="weui-panel ">
	<navigator url="/pages/store/store" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
		<view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
			<image class="weui-media-box__thumb" src="/images/logo2.png" />
		</view>
		<view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
			<view class="weui-media-box__title weui-cell__ft_in-access">{{store.name}}</view>
			<view class="weui-media-box__desc">{{store.description}}</view>
		</view>
	</navigator>
</view>
<view class="weui-panel weui-media-box">
	<block wx:for="{{pagination.content}}" wx:for-item='product' wx:key="index">
		<navigator url="/pages/store/detail" class=" weui-media-box_appmsg" hover-class="weui-cell_active">
			<view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
				<view class="weui-badge">1</view>
				<image class="weui-media-box__thumb" src="{{prefix}}{{product.pictures[0]}}" />
			</view>
			<view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
				<view class="weui-media-box__title">{{product.name}}</view>
				<view class="weui-media-box__desc">{{product.description}}</view>
				<view class="weui-media-box__desc"><label class="weui-cell_warn">{{product.price}}{{product.priceUnit}}</label></view>
			</view>
		</navigator>
		<view class="weui-cell__ft">
			<button type="default" size="mini">减</button>
			<button type="default" size="mini" disabled="true">1</button>
			<button type="default" size="mini">加</button>
		</view>
	</block>
